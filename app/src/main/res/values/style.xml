        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/nunito_regular</item>
    </style>

    <style name="AppNoteTitleView" parent="Widget.AppCompat.EditText">
        <item name="android:textColor">#FFFFFF</item>
